{
    "presets": [
        ["env",{ "modules": false }], "react"
    ],
   
    "plugins": [
        [  
            "transform-class-properties",
            "transform-es2015-classes", {
                "loose": true
            }
        ],
        ["transform-object-rest-spread", { "useBuiltIns": true }],
        ["module-resolver", {
           "root": ["."],
           "alias": {
             "react-core":"./src/core"
           }
         }]
    ]
}